procedure par()
  if 0 < n then
    n := n - 1;
    impar()
  else
    res := true
  end
end;

procedure impar()
  if 0 < n then
    n := n - 1;
    par()
  else
    res := false
  end
end;

var x: int, n: int, res, true, false: bool;
true := 0 = 0;
false := 0 = 1;
read x;
n := x;
par();
write res;
n := x;
impar();
write res
